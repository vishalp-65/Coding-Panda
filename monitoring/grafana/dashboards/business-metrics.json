{
  "dashboard": {
    "id": null,
    "title": "AI Coding Platform - Business Metrics",
    "tags": ["platform", "business", "kpi"],
    "style": "dark",
    "timezone": "browser",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 50}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Problems Solved (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(problems_solved_total[24h])",
            "legendFormat": "Problems Solved"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Code Executions (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(code_executions_total[24h])",
            "legendFormat": "Code Executions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Active Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "user_sessions_active",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 20}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Problems Solved by Difficulty",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(problems_solved_total[5m])) by (difficulty)",
            "legendFormat": "{{difficulty}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Code Executions by Language",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(code_executions_total[5m])) by (language)",
            "legendFormat": "{{language}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Contest Participation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "contest_participants_total",
            "legendFormat": "Contest {{contest_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "AI Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(ai_request_duration_seconds_count[5m])) by (operation)",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "Success Rate by Operation",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{status=~\"2..\"}[5m])) by (route) / sum(rate(http_requests_total[5m])) by (route)) * 100",
            "legendFormat": "{{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Code Execution Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "(sum(rate(code_executions_total{status=\"success\"}[5m])) / sum(rate(code_executions_total[5m]))) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ]
  },
  "overwrite": true
}