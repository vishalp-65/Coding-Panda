# Server Configuration
NODE_ENV=development
PORT=3000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
RATE_LIMIT_SKIP_SUCCESSFUL=false

# Service URLs
USER_SERVICE_URL=http://localhost:3006
USER_SERVICE_TIMEOUT=5000
PROBLEM_SERVICE_URL=http://localhost:3002
PROBLEM_SERVICE_TIMEOUT=5000
EXECUTION_SERVICE_URL=http://localhost:3003
EXECUTION_SERVICE_TIMEOUT=30000
AI_SERVICE_URL=http://localhost:3004
AI_SERVICE_TIMEOUT=10000
CONTEST_SERVICE_URL=http://localhost:3005
CONTEST_SERVICE_TIMEOUT=5000

# CORS Configuration
CORS_ORIGIN=*
CORS_CREDENTIALS=true